{
  "name": "audio-visual-3d",
  "private": true,
  "version": "0.1.0",
  "license": "MPL-2.0",
  "type": "module",
  "engines": {
    "node": ">=20.12.0 <25.0.0 || ^24.0.0"
  },
  "scripts": {
    "prepare": "node scripts/configure-hooks.mjs",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --strictPort --port 5173",
    "start": "node server/index.js",
    "server": "tsc --project server/tsconfig.json && node server/index.js",
    "generate-notices": "node scripts/generate-notices.js",
    "test": "vitest run",
    "validate:schemas": "node scripts/validate-schemas.mjs",
    "demo": "make demo",
    "evidence": "node scripts/evidence.mjs",
    "lint": "node scripts/run-eslint.mjs",
    "lint:fix": "node scripts/run-eslint.mjs --fix",
    "format": "node scripts/run-prettier.mjs --fix",
    "format:check": "node scripts/run-prettier.mjs --check",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:integration": "vitest run tests/integration --passWithNoTests",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "package": "npm pack --pack-destination artifacts",
    "release": "node scripts/release-notes.mjs",
    "docs:build": "node scripts/gen-typedoc.mjs",
    "update-deps": "node scripts/update-deps.mjs",
    "security:scan": "node scripts/security-scan.mjs && npm audit --audit-level=moderate || echo \"npm audit failed (offline?)\"",
    "sbom": "mkdir -p sbom && node scripts/generate-sbom.mjs",
    "migrate": "echo 'No migrations defined'",
    "check": "node scripts/run-check.mjs",
    "commitlint": "node scripts/commitlint.mjs"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.28.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "three-stdlib": "^2.36.0"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^4.0.0",
    "@google/generative-ai": "^0.24.1",
    "@lit-labs/ssr": "^3.3.1",
    "@lit-labs/ssr-client": "^1.1.7",
    "@lit/reactive-element": "^2.1.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/seedrandom": "^3.0.8",
    "@types/three": "^0.179.0",
    "@types/web": "^0.0.265",
    "@types/webrtc": "^0.0.46",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "ajv": "^8.17.1",
    "license-checker": "^25.0.1",
    "lit": "^3.3.1",
    "playwright": "^1.48.2",
    "seedrandom": "^3.0.5",
    "three": "^0.179.1",
    "typescript": "~5.3.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "description": "Interactive audio-to-visual 3D exploration experience."
}
